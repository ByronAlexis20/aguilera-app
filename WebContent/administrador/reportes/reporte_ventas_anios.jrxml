	<queryString language="SQL">
		<![CDATA[SELECT 	YEAR(c.fecha_venta) AS anio,
		SUM(c.total) AS valor
FROM da_cabecera AS c
WHERE 	c.estado IS NULL AND
		YEAR(c.fecha_venta) >= $P{__ANIO_DESDE__}  AND
        YEAR(c.fecha_venta) <= $P{__ANIO_HASTA__} 
GROUP BY YEAR(c.fecha_venta);]]>
